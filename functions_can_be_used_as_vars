import math 
import matplotlib.pyplot as plt 

def f(x):
    return math.log((x+(x+1)+(x+2))**x)/math.log(math.e)

a = f
xs = [i for i in range(1, 1000)]
ys = [a(x) for x in xs]
xs2 = [i for i in range(1, 1000)]
ys2 = [i*math.log(i)/math.log(math.e) for i in xs2] 
plt.plot(xs, ys)
plt.plot(xs2, ys2)
plt.xlabel("x")
plt.ylabel("f(x)")
plt.grid(True)
plt.show()
